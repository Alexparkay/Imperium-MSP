{
  "name": "imperium-msp",
  "version": "1.0.0",
  "description": "Modern dashboard application with glassmorphic dark UI design",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:frontend",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "preview": "cd frontend && npm run preview",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules frontend/dist backend/dist",
    "deploy:frontend": "cd frontend && vercel --prod",
    "deploy:backend": "cd backend && vercel --prod"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "keywords": [
    "dashboard",
    "react",
    "typescript",
    "vite",
    "tailwind",
    "mui",
    "glassmorphism",
    "dark-theme"
  ],
  "author": "Alex P",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/imperium-msp.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@splinetool/react-spline": "^4.0.0",
    "@splinetool/runtime": "^1.9.92",
    "apexcharts": "^4.5.0",
    "d3-geo": "^3.1.1",
    "react-apexcharts": "^1.7.0",
    "react-globe": "^5.0.2",
    "react-simple-maps": "^3.0.0",
    "react-usa-map": "^1.5.0",
    "three": "^0.176.0",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "@types/react-simple-maps": "^3.0.6",
    "@types/topojson-client": "^3.1.5"
  }
}
